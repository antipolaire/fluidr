#version 410

precision highp float;

uniform sampler2D iChannel1;

uniform sampler2D iChannel2;

in vec2 vertTexCoord0;

uniform vec3 iResolution;

out vec4 fragColor;

uniform float uTime = 0.0;

// Pressure solver

float p0c(int x,int y, vec2 C){
    return texelFetch(iChannel2, ivec2(C)+ivec2(x,y), 0).x;
}

float rhs(int x,int y, vec2 C){
    vec2 cc = C + vec2(x,y);
    return  texelFetch(iChannel1,ivec2(cc),0).x *
            (1.-step(iResolution.x, cc.x)) *
            (1.-step(iResolution.y, cc.y)) *
            (step(0., cc.x)) *
            (step(0., cc.y));
}


